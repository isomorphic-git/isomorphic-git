{
  "source": "./src/commands",
  "destination": "./doc",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./__tests__/",
          "interfaces": ["describe", "test"],
          "includes": ["^test-(.*)\\.js$"]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "stripPackageName": true,
        "replaces": [
          {"from": ".*", "to": "isomorphic-git"}
        ]
      }
    }
  ]
}
