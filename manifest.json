		"assets",
		"file",
		"http",
		"preference",
		"scroller",
		"mod/inflate",

		"api/*",
		"commands/*",
		"errors/*",
		"models/*",
		"managers/*",
		"storage/*",
		"utils/*",
		"wire/*",
	],
	"platforms": {
		"esp": {},
		"esp32": {
			"config": {
				"rotation": 90
			}
		},
		"qca4020": {},
		"mac": {
			"config": {
				"skipwifi": true
			}
		},
		"...": {
			"error": "WiFi module unsupported"
		}
	},
	"config": {
		"brightness": 50
	},
	"creation": {
		"static": 98304,
		"stack": 1024,
		"keys": {
			"available": 32,
		},
	},
	"resources": {
		"*-alpha": [
			"$(MODDABLE)/examples/assets/fonts/OpenSans-Semibold-16",
			"$(MODDABLE)/examples/assets/fonts/OpenSans-Semibold-18",
			"$(MODDABLE)/examples/assets/fonts/OpenSans-Regular-20",
			"$(MODDABLE)/examples/assets/fonts/OpenSans-Regular-16",
		],
		"*-mask": [
